<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ethan.pokerjournal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".GameFormActivity" />
        <activity android:name=".GameDetailActivity" />
        <activity android:name=".GameEditActivity" />
        <activity android:name=".BankFormActivity" />
        <activity android:name=".BankDetailActivity" />
        <activity android:name=".BankEditActivity" />
        <activity android:name=".StatsGraphActivity" />
    </application>

</manifest>